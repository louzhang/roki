@model List<DiscordGuild>
<h3>My Servers</h3>

<div class="container">
    <div class="row justify-content-start">
        @foreach (DiscordGuild guild in Model)
        {
            <div class="col-2">
                <div class="card">
                    <div class="card-body">
                        <p>@guild.Name</p>
                    </div>
                </div>
            </div>
        }
    </div>
</div>